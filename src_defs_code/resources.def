def Resource
{
	text name
	text tooltip
	sprite icon = "Assets/UI/Elements/Common/Icons/RealmResources/Icon_Resource_Placeholder.png:Icon_Resource_Placeholder#2" //GUID:3d5e963fb3b44544ba59e318c41a7af5
	text category = "Base"
	
	float import_gold = 90000
	float import_commerce = 190000 // overriden per resource
	float import_commerce_mul = 5
	float building_commerce_upkeep_mul = 0.5 // building upkeep if resource is missing = import_commerce * this
	
	import_upkeep
	{
		gold = (ceil( import_gold * (1.3 ^ (-relationship / 1000))))
		commerce = (ceil(import_commerce * import_commerce_mul) + (distance-1) * 3) // * distance_weight
	}
	
	availability_texts
	{
		text Available_1 = "We are currently producing {availability_color}{resource.name}{/availability_color} in {availability_castle_1}."
		text Available_2 = "We are currently producing {availability_color}{resource.name}{/availability_color} in {availability_castle_1} and {availability_castle_2}."
		text Available_3 = "We are currently producing {availability_color}{resource.name}{/availability_color} in {availability_castle_1}, {availability_castle_2} and {availability_castle_3}."
		text Available_many = "We are currently producing {availability_color}{resource.name}{/availability_color} in {availability_castles_count} of our provinces ({availability_castle_1}, {availability_castle_2}, {availability_castle_3} and others)."
		text Available_all = "We are currently producing {availability_color}{resource.name}{/availability_color} in all of our our provinces."
		
		text DirectlyObtainable_1 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1}."
		text DirectlyObtainable_2 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1} and {availability_castle_2}."
		text DirectlyObtainable_3 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1}, {availability_castle_2} and {availability_castle_3}."
		text DirectlyObtainable_many = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castles_count} of our provinces ({availability_castle_1}, {availability_castle_2}, {availability_castle_3} and others)."
		text DirectlyObtainable_all = "We can produce {availability_color}{resource.name}{/availability_color} in all of our our provinces."
		
		text IndirectlyObtainable_1 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1}, but we need to develop other provinces first."
		text IndirectlyObtainable_2 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1} and {availability_castle_2}, but we need to develop other provinces first."
		text IndirectlyObtainable_3 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1}, {availability_castle_2} and {availability_castle_3}, but we need to develop other provinces first."
		text IndirectlyObtainable_many = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castles_count} of our provinces ({availability_castle_1}, {availability_castle_2}, {availability_castle_3} and others), but we need to develop other provinces first."
		text IndirectlyObtainable_all = "We can produce {availability_color}{resource.name}{/availability_color} in all of our our provinces, but we need to develop some provinces first."
		
		text Impossible_0 = "We cannot produce {availability_color}{resource.name}{/availability_color} in any of our provinces."
		text Impossible_1 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1}, but we need to gain access to goods outside our kingdom first."
		text Impossible_2 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1} and {availability_castle_2}, but we need to gain access to goods outside our kingdom first."
		text Impossible_3 = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castle_1}, {availability_castle_2} and {availability_castle_3}, but we need to gain access to goods outside our kingdom first."
		text Impossible_many = "We can produce {availability_color}{resource.name}{/availability_color} in {availability_castles_count} of our provinces ({availability_castle_1}, {availability_castle_2}, {availability_castle_3} and others), but we need to gain access to goods outside our kingdom first."
		text Impossible_all = "We can produce {availability_color}{resource.name}{/availability_color} in all of our our provinces, but we need to gain access to goods outside our kingdom first."
	}
}

