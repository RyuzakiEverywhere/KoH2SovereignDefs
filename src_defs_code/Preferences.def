	template Preference
	{
		text name
		text description
		bool instant_apply = false
		bool reqire_restart = false
		bool allowed_in_singleplayer = true
		bool allowed_in_multiplayer = true
		text restart_warning = "This option reqires the game to be restarted"
		color color_label_normal = "255,240,217" //FFF0D9FF
		color color_label_disabled = "139,129,115" //8C8273FF

		keybind_text_color
		{
			color normal = "149,123,94,255" //957B5E
			color focus = "255,240,217,255" //FFF0D9
		}
	}
	
	template Confirmation
	{
		int timeout = 0 //seconds
		text caption = "Confirmation"
		text body = "Are you sure you want to change this setting?"
		text confirm = "Confirm"
		text cancel = "Cancel"
	}

	template KeyBindConfirmation : Confirmation
	{
		caption = "Duplicate hotkey"
		body = "Are you sure you wish to assign this key here? It's currently used in other fields, which will be left empty."
	}
	
	category Gameplay
	{
		text name = "Gameplay"
		settings : category_settings
		
		label general
		{
			text name = "General"
		}
		
		dropdown difficulty : Preference
		{
			name = "Difficulty"
			description = "Determines various gameplay factors, such as the amount of starting wars, how many rebels spawn in the world, how fast other kingdoms gain resources and so on.[ {allowed_in_multiplayer:if}|{is_multiplayer:if}{p}{clr:red}You are not allowed to adjust the difficulty during a multiplayer game.{/clr}]"
			allowed_in_multiplayer = false
		}
		toggle autosave : Preference
		{
			name = "Periodic Autosave"
			description = "Turn periodic autosaving on/off."
		}
		toggle pause_on_out_of_focus : Preference
		{
			name = "Pause on Alt-Tab"
			description = "Pause the game when you alt-tab out of the game or lose focus on the game window. Works only in singleplayer."
			allowed_in_multiplayer = false
		}
		toggle tutorial_messages : Preference
		{
			allowed_in_multiplayer = false
			name = "World Map Tutorials"
			description = "Enable/disable all general tutorial messages, outside of tactical battles. These tutorials do not activate in multiplayer."
		}
		toggle tutorial_messages_battles : Preference
		{
			allowed_in_multiplayer = false
			name = "Tactical Battle Tutorials"
			description = "Enable/disable tutorial messages during tactical battles."
		}
		button reset_tutorials : Preference
		{
			allowed_in_multiplayer = false
			name = "Reset Tutorials"
			description = "Reset all tutorial sequences that you've seen so far."
			text button_label = "Reset"
			confirmation : Confirmation
			{
				caption = "Reset tutorials"
				body = "Are you sure you wish to reset all tutorials? Already seen tutorial sequences will start to appear once again."
			}
		}
		
		// toggle left_mouse_map_drag : Preference
		// {
		// 	name = "Left mouse map drag"
		// 	description = "Enable/disable camera panning by holding down LMB."
		// }
		dropdown reminders : Preference
		{
			name = "Reminder messages"
			description = "Limit certain messages with lower importance, such as reminders for kingdom issues and unused resources."
		}
		
