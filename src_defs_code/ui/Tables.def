def Table
{
	prefab prefab = "Assets/UI/Table/UIP_GenericTable.prefab" //GUID:81a91b1761e2ddb4f93a6337c9b826ad
	sprite background
	
	text title // the header of the table
	text tooltip // when hovering on top of the title
	int window_height = 750 // max height (optional, otherwise dynamically resized based on max_rows)
	int max_rows = 10 //amount of rows after whitch scrolling might occur
	
	bool autosave_settings = false // = true, if we want to save the window's scroll position/ordering, once it's reopened
	column_list // {} - ordered by the way they are visualized
	string default_sorting_column // {} - list of columns by which to sort, ordered by priority
	bool show_header = true
	
	prefab row_prefab = "Assets/UI/Table/UIP_GenericTableRow.prefab" //GUID:e9e55041932149344802b495fa9ab2c4
	
}

//...
def TableRow
{
	prefab prefab = "Assets/UI/Table/UIP_GenericTableRow.prefab" //GUID:e9e55041932149344802b495fa9ab2c4
	prefab empty_prefab = "Assets/UI/Table/UIP_GenericTableRow_Placehoder.prefab" //GUID:6cae7e21ce44f32419de81eb0ed08371
	float horizontal_spacing = 4 // between columns
	float vertical_spacing = 4 // between rows
	
	sprite odd_line_sprite
	sprite even_line_sprite
	
	color odd_line_color
	color even_line_color
	
	padding
	{
		float top = 0
		float left = 0
		float right = 0
		float bottom = 0
	}
	
	// rename to Formatting
	template Formatting
	{
		color tint = "45,40,40,255"
		sprite background
	}
	odd
	{
		Normal : TableRow.Formatting
		{
			tint = "45,0,0" //2D2828FF
		}
		Hover : TableRow.Formatting
		{
			tint = "74,66,66" //4B4343FF
		}
		Selected : TableRow.Formatting
		{
			tint = "92,82,82" //5D5353FF
		}
		Disbaled : TableRow.Formatting
		{
			tint = "14,12,12" //0E0D0DFF
		}
	}
	even
	{
		Normal : TableRow.Formatting
		{
			tint = "50,50,50" //2D2828FF
		}
		Hover : TableRow.Formatting
		{
			tint = "74,66,66" //4B4343FF
		}
		Selected : TableRow.Formatting
		{
			tint = "92,82,82" //5D5353FF
		}
		Disbaled : TableRow.Formatting
		{
			tint = "14,12,12" //0E0D0DFF
		}
	}
}

def TableColumn
{
	// shared 
	bool hide_if_empty = false
	float width // static, always as set
	string sort_method // ascending, descendingg
	float update_interval = 1 //in secounds, 0 = never
	float row_height = 36 // global 
	float header_height = 30 // global 
	
	// header data 
	text title = "Column title"
	text header_tooltip_caption = ""
	text header_tooltip_body = ""
	
	sprite icon //if we want to append a small icon to the LEFT of cell
	sprite header_icon // if we want to append a small icon to the LEFT of column heeadwer
	sprite header_background // override background sprite
	sprite header_background_over
	sprite header_background_select
	prefab header_prefab = "Assets/UI/Table/UIP_GenericTableHeader.prefab" //GUID:9082dc3370674eb438bdb5bb83df812b
	
	// cell data 
	sprite cell_icon //if we want to append a small icon to the LEFT of EVERY CELL
	prefab cell_prefab = "Assets/UI/Table/UIP_GenericTableCell.prefab" //GUID:34a4523ac5b8bf149b6a142b45059a49
	
	// TODO: change to "cell_text"
	text cell_value = "{obj}"
	cell_icon_value = (obj)
	{
		float width = 0 //0 = fit in parent
		float height = 0
	}
	text text = "{obj}"
	
	template Formatting
	{
		// font size ?
		padding
		{
			float top = 0
			float left = 0
			float right = 0
			float bottom = 0
		}
		
		string alignment = "MiddleCenter" { enum { UpperLeft; UpperCenter; UpperRight; MiddleLeft; MiddleCenter; MiddleRight; LowerLeft; LowerCenter; LowerRight } }
		
		color tint = "255,255,255,255"
		color text_tint = "138,132,118,255"
		float text_size = 18
		bool auto_sizing = true
	}
	
	header_formatting
	{
		Normal : TableColumn.Formatting
		Hover : TableColumn.Formatting
		Selected : TableColumn.Formatting
		Disbaled : TableColumn.Formatting
	}
	
	cell_formatting
	{
		Normal : TableColumn.Formatting
		Hover : TableColumn.Formatting
		Selected : TableColumn.Formatting
		Disbaled : TableColumn.Formatting
	}
	
}


