def UIBattleViewArmyCommandsWindow : UIWindow
{
	window_prefab = "Assets/UI/BattleView/UIP_BattleView_ArmyCommandsWindow.prefab" //GUID:3429469c1eae7f14180e80745ebe2328
	prefab command_binding_prefab = "Assets/UI/BattleView/UIP_ArmyCommandBinding.prefab" //GUID:0d3d8550d39fbfe418f05f03ee55a940
	text title = "Army Commands"
	
	sprite mouse_button_0 = "Assets/UI/BattleView/Elements/BV_UI_Element_LMB.psd:BV_UI_Element_LMB#1" //GUID:b7f11369b8c92a8489876ee55a3393cc
	sprite mouse_button_1 = "Assets/UI/BattleView/Elements/BV_UI_Element_RMB.psd:BV_UI_Element_RMB#2" //GUID:1c7c3185b3d7eb4409b79b8692864bc6
