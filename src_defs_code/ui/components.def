	template CommandTemplate
	{
		text command_text
		string name_of_keybind
		text tooltip
	}
	Commands // Keybinds that will be displayed in the army commands window
	{
		// Contains commands that binding cannot be changed
		// Use MouseX (where X belongs to <0:2>) to get the specify mouse button
		Hardcoded
		{
			MeleeAttack : CommandTemplate
			{
				command_text = "Attack/Move"
				name_of_keybind = "Mouse1"
				tooltip = "Order selected squad(s) to move to a location or attack a target. Hold and drag {clr:gold}RMB{/clr} to arrange selected troops' formations."
			}
			ForceMeleeAttack : CommandTemplate
			{
				command_text = "Force melee attack"
				name_of_keybind = "CTRL-Mouse1"
				tooltip = "Order selected ranged squad(s) to attack a target from melee distance."
			}
			DragFromCenter : CommandTemplate
			{
				command_text = "Centered formation move" //"Drag from center"
				name_of_keybind = "{Settings.alternate_drag_formation.in_ui}-Mouse1"
				tooltip = "Order selected squad(s) to move to a location, retaining their formations and positioning themselves in a line, extending from the center of the mouse cursor."
			}
			DragAndKeepOffset : CommandTemplate
			{
				command_text = "Preserved formation move" //"Drag formation and keep offset"
				name_of_keybind = "{Settings.alternate_drag_formation.in_ui}-Mouse0"
				tooltip = "Order selected squad(s) move to a location, retaining the spacing between themselves and the orientation of each individual squad."
			}
			ChargeMouse : CommandTemplate
			{
				command_text = "Run/Charge to target"
				name_of_keybind = "Mouse1-Mouse1"
				tooltip = "Order selected squad(s) to run. Squads which are currently ordered to Attack an opponent will attempt to charge them."
			}
		}
		// Contains commands that binding can be found in Preferences.def
		Assignable
		{
			Charge : CommandTemplate
			{
				command_text = "Run/Charge"
				name_of_keybind = "toggle_squad_charge"
				tooltip = "Order selected squad(s) to run to a location or charge an opponent."
			}
			Stop : CommandTemplate
			{
				command_text = "Stop moving"
				name_of_keybind = "toggle_squad_stop"
				tooltip = "Order selected squad(s) to stop performing their current action, if possible."
			}
			ShrinkRanks : CommandTemplate
			{
				command_text = "Shrink Ranks"
				name_of_keybind = "toggle_squad_shrink_ranks"
				tooltip = "Order all units in selected squad(s) to move closer to each other."
			}
			RegularRanks : CommandTemplate
			{
				command_text = "Regular Ranks"
				name_of_keybind = "toggle_squad_regular_ranks"
				tooltip = "Order all units in selected squad(s) to regain their regular spacing."
			}
			WidenRanks : CommandTemplate
			{
				command_text = "Widen Ranks"
				name_of_keybind = "toggle_squad_widen_ranks"
				tooltip = "Order all units in selected squad(s) to increase the distance between each other."
			}
			SquareFormation : CommandTemplate
			{
				command_text = "Square Formation"
				name_of_keybind = "toggle_squad_square_formation"
				tooltip = "Order selected squad(s) to form a square formation."
			}
			TriangleFormation : CommandTemplate
			{
				command_text = "Triangle Formation"
				name_of_keybind = "toggle_squad_triangle_formation"
				tooltip = "Order selected squad(s) to form a wedge formation."
			}
			HoldGround : CommandTemplate
			{
				command_text = "Preserve Formation (toggle)"
				name_of_keybind = "toggle_squad_hold_ground"
				tooltip = "Order selected squad(s) to act more passively or aggressively. Troops in Preserve Formation state will only engage enemies very close to them, try to stay in their spot and break squad ranks less when engaged in hand-to-hand combat."
			}
			HoldFire : CommandTemplate
			{
				command_text = "Hold Fire (toggle)"
				name_of_keybind = "toggle_squad_hold_fire"
				tooltip = "Forbid/allow selected ranged squad(s) to fire at opponents. Direct attack orders override this state."
			}
			Deploy : CommandTemplate
			{
				command_text = "Deploy/Pack Trebuchet"
				name_of_keybind = "toggle_squad_deploy_trebuchet"
				tooltip = "Order selected trebuchet(s) to deploy/pack. Trebuchets can't fire until fully deployed, and can't move until fully packed."
			}
			MarkAsTarget : CommandTemplate
			{
				command_text = "Mark Target"
				name_of_keybind = "toggle_mark_as_target"
				tooltip = "Mark selected enemy squad(s) as priority target(s) for ranged squads."
			}
			FocusOnSelection : CommandTemplate
			{
				command_text = "Focus on selection"
				name_of_keybind = "focus_selection"
				tooltip = "Move the camera to the central point of all selected squad(s)."
			}
			RevealTravelPaths : CommandTemplate
			{
				command_text = "Reveal army selections/travel paths"
				name_of_keybind = "hold_to_reveal_travel_paths"
				tooltip = "Hold the assigned button to reveal all squad selection indications, in addition to travel paths and formations of squads on your side."
			}
		}
	}
}

